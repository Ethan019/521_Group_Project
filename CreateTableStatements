CREATE TABLE StudentRecord(
  RECORDID NUMERIC PRIMARY KEY,
  STUDID FOREIGN KEY REFERENCES Student(STUDID),
  COURSENUM INT NOT NULL DEFAULT 'U000',
  GRADE CHAR DEFAULT '-',
  SEMESTER STRING (CHECK SEMESTER == "FALL" OR SEMESTER == "SPRING"),
  YEAR INT NOT NULL (CHECK YEAR !< 2000 AND YEAR !> 3000)
  );

